<form class="pl1e" autocomplete="off" onsubmit="event.preventDefault();">
    <div class="list-items">
        {{#each choices}}
            <div class="item">
                <div class="title">
                    <label>{{localize label}}</label>
                </div>
                <div class="fields">
                    <input type="checkbox" name="{{@key}}" {{checked value}} data-dtype="Boolean">
                </div>
            </div>
        {{/each}}
    </div>
    <button type="submit"><i class="fas fa-save"></i> {{localize "PL1E.Save"}}</button>
</form>
